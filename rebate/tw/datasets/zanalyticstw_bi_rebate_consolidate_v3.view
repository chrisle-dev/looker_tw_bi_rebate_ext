    label: " "
    sql: "Grand Total" ;;
  }
  dimension: customer_clean_dim {
    type: string
    sql: ${TABLE}.customer_clean_dim ;;
  }
  dimension: test {
    type: number
    sql: ${TABLE}.test ;;
  }
  dimension: test2 {
    type: number
    sql: ${TABLE}.test2 ;;
  }
  dimension: mau_so {
    type: number
