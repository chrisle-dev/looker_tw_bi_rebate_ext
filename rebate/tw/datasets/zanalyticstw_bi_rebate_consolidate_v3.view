           AND EXTRACT(YEAR FROM ${TABLE}.bi_update_date) = EXTRACT(YEAR FROM CURRENT_DATE())
         THEN IFNULL(${rebate_amt_exc_vat}, 0) * 1.05
         ELSE 0
